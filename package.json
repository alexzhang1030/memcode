{
  "name": "memcode",
  "type": "module",
  "version": "1.2.1",
  "packageManager": "pnpm@9.15.1",
  "description": "Organize your filles and download them in your browser.",
  "author": "alexzhang1030",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhang1030/memcode.git"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "dev": "tsup --watch",
    "lint": "NODE_OPTIONS=--import=tsx eslint --config=eslint.config.ts .",
    "prepare": "simple-git-hooks",
    "build": "tsup",
    "prepublishOnly": "pnpm run build",
    "release": "vitest run && bumpp --commit \"chore: release v%s\" --push --tag && pnpm publish",
    "play": "pnpm -F playground dev"
  },
  "dependencies": {
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "to-path-tree": "^1.2.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.12.0",
    "@types/file-saver": "^2.0.7",
    "bumpp": "^9.9.2",
    "eslint": "^9.17.0",
    "lint-staged": "^15.2.11",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "pnpm lint --fix"
    ]
  }
}
